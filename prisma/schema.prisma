// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Currency {
  id              String @id @default(cuid())
  name            String?
  symbol          String
  amount          String
  url             String @unique
  slug            String @unique
  userId          String
  createdAt       DateTime @default(now())

  @@map("currencies")
}
